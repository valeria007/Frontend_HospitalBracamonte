     <!--Contenido-->
     <div class="contentpanel"><br>
      <!--Formulario-->
      <div class="container clear_both padding_fix">
        <div class="row">
          <div class="col-md-12">
            <div class="block-web">
              <div class="header">
                <div class="actions"><a class="minimize" href="#"><i class="fa fa-chevron-down"></i></a> <a class="refresh" href="/almacen/volver1/<%=data_doc.data.token_id%>/<%=data_doc.data.token_part%>"><i class="fa fa-repeat"></i></a> </div>
                <h2 class="content-header"><div class="info gray_symbols"><i class="fa fa-list icon"></i></div>   Registrar Grupo de Asignación</h2>
              </div>
              <div class="porlets-content">
                
                <br>
                <br>
                <%if (oneGrupoAsig == null) { %>
                <form  class="form-horizontal group-border-dashed" action="/almacen/grupoAsig/<%=data_doc.data.token_id%>/<%=data_doc.data.token_part%>" method="POST" parsley-validate novalidate onkeyup="mayus(this);">
                  <input type="hidden" name="id_personal" value="<%=data_doc.data.personal.id%>">
                <%if(err != null) { %>
                  
                  <div class="form-group">
                    <label class="col-sm-3 control-label">Codigo</label>
                    <div class="col-sm-6">                   
                      <input type="number" class="form-control" required parsley-minlength="3"  name="codigo" value="<%=err.data.codigo%>" onkeyup="mayus(this);"/>
                    </div>
                  </div><!--/form-group--> 
                  <div class="form-group">
                    <label class="col-sm-3 control-label">Descripcion</label>
                    <div class="col-sm-6">                   
                      <input type="text" class="form-control" required parsley-minlength="6" name="descripcion" value="<%=err.data.descripcion%>" onkeyup="mayus(this);"/>
                    </div>
                  </div><!--/form-group--> 
                  <div class="form-group">
                    <div class="col-sm-offset-1 col-sm-10">
                      <button  type="submit" class="btn btn-success" ><i class="fa fa-floppy-o"></i>  Guardar</button>
                      <a href="/almacen/quitar/<%=data_doc.data.token_id%>/<%=data_doc.data.token_part%>">Cancelar</a>
                      
                    </div>
                  </div><!--/form-group--> 
                <% } else { %>
                  <div class="form-group">
                    <label class="col-sm-3 control-label">Codigo</label>
                    <div class="col-sm-6">                       
                      <input type="number" class="form-control" required parsley-minlength="3"  name="codigo" onkeyup="mayus(this);"/>
                    </div>
                  </div><!--/form-group--> 
                  <div class="form-group">
                    <label class="col-sm-3 control-label">Descripcion</label>
                    <div class="col-sm-6">
                      <input type="text" class="form-control" required parsley-minlength="6"  name="descripcion" onkeyup="mayus(this);"/>
                    </div>
                  </div><!--/form-group--> 
                  <div class="form-group">
                    <div class="col-sm-offset-1 col-sm-10">
                      <button  type="submit" class="btn btn-success" ><i class="fa fa-floppy-o"></i>  Guardar</button>
                      <button type="reset" value="reset" class="btn btn-danger"><i class="fa fa-times"></i> Cancelar</button>
                    </div>
                  </div><!--/form-group--> 
                <% } %>
                 
                </form>
                <% } else { %>
                <form class="form-horizontal group-border-dashed" action="/almacen/updateGPA/<%=oneGrupoAsig.data[0].id%>/<%=data_doc.data.token_id%>/<%=data_doc.data.token_part%>" method="POST" parsley-validate novalidate>
                  <div class="form-group">
                    <label class="col-sm-3 control-label">Codigo:</label>
                    <div class="col-sm-6">
                      <input type="number" class="form-control" value="<%=oneGrupoAsig.data[0].codigo%>" required parsley-minlength="3" readonly onkeyup="mayus(this);"/>
                    </div>
                  </div><!--/form-group--> 
                  <div class="form-group">
                    <label class="col-sm-3 control-label">Descripcion:</label>
                    <div class="col-sm-6">
                      <input type="text" class="form-control" value="<%=oneGrupoAsig.data[0].descripcion%>"  required parsley-minlength="6" name="descripcion" onkeyup="mayus(this);"/>
                    </div>
                  </div><!--/form-group--> 
                  <div class="form-group">
                    <div class="col-sm-offset-1 col-sm-10">
                      <button type="submit" class="btn btn-success"><i class="fa fa-floppy-o"></i>  Guardar</button>
                      <a type="reset" class="btn btn-danger" href="/almacen/volver1/<%=data_doc.data.token_id%>/<%=data_doc.data.token_part%>"><i class="fa fa-times"></i> Cancelar</a>
                    </div>
                  </div>
                </form>
                <% } %>
              </div><!--/block-web--> 
              <div id="main-content">
                <div class="page-content">
                  <div class="row">
                    <div class="col-md-12">
                      <div class="block-web">
                        <div class="header">
                          <div class="actions"> <a class="minimize" href="#"><i class="fa fa-chevron-down"></i></a> </div>
                          <h3 class="content-header">Lista de Grupo de Asignación</h3>
                        </div>
                        <div class="porlets-content">
                          <div class="table-responsive">
                            <table  class="display table table-bordered table-striped " id="dynamic-table">
                              <thead>
                                <tr>
                                <th>#</th>
                                <th>Codigo</th>
                                <th>Descripcion</th>
                                <th >Modificar</th>
                                </tr>
                              </thead>
                              <tbody>
                                <% for(var i=0; i < resp.length; i++) { %>
                                <tr>
                                <td><%= i+1 %></td>
                                <td><%=resp[i].codigo%></td>
                                <td><%=resp[i].descripcion%></td>
                                <td class="center hidden-phone">
                                <a href="/almacen/GrupoAsigONLY/<%=resp[i].id%>/<%=data_doc.data.token_id%>/<%=data_doc.data.token_part%>" data-toggle="tooltip" class="btn  tooltips" data-original-title="Actualizar"><i class="fa fa-refresh "></i></a><span>&nbsp;</span>
                                </td>
                                </tr>
                                <% } %>
                              </tbody>
                            </table>
                          </div><!--/table-responsive-->
                        </div><!--/porlets-content-->
                      </div><!--/block-web--> 
                    </div><!--/col-md-12--> 
                  </div><!--/row-->
                </div><!--/page-content end--> 
              </div>
            </div>
          </div>
        </div><!--/row-->
      </div>
    </div>
  </div>
</div>
<%- include("../partials/scripts") -%>

