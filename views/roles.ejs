<script>
  // No conflict for WordPress
var $j = jQuery.noConflict();

$j( function() {
	$j( ".date-picker-min" ).datepicker({
	  dateFormat: "yy-mm-dd"
	});
	$j( ".date-picker-max" ).datepicker({
	  dateFormat: "yy-mm-dd"
	});
});

/* DataTable */
$j(document).ready(function(){

    var table2 = $j('.datatables-users-quiz-table').DataTable({
		"order": [[ 0, "asc" ]],
		"paging": false,
		"bLengthChange": false,
		"bInfo": false,
		"dom": 'Bfrtip',
		"buttons": [
			'copyHtml5',
			'excelHtml5', // deactivated because JSZip dependency
			'csvHtml5',
			'pdfHtml5' // deactivated because PDFMake dependency
		]
	});

  // https://datatables.net/reference/type/column-selector
  $j(
    ".filters-groups .date-picker-max, .filters-groups .date-picker-min"
  ).change(function() {
    var val = parseInt((new Date(this.value).getTime() / 1000).toFixed(0));

    var op = "min";
    if ($j(this).hasClass("date-picker-max")) {
      op = "max";
    }

    table2.columns().every(function() {
      var header = this.header();
      var date = $j(header).data("filter");
      if (date) {
        if (op === "min") {
          if (date < val) {
            this.visible(false);
          } else {
            this.visible(true);
          }
        } else {
          if (date > val) {
            this.visible(false);
          } else {
            this.visible(true);
          }
        }
      }
    });

    // column.visible( ! column.visible() );

    table2.draw();
  });


});

</script>
