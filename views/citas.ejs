							<!--/main-content end-->



							<!-- Modal -->
							<div class="modal fade" id="myModal" tabindex="-1" role="dialog"
								aria-labelledby="myModalLabel" aria-hidden="true">
								<div class="modal-dialog">
									<div class="modal-content">
										<div class="modal-header">
											<button type="button" class="close" data-dismiss="modal"
												aria-hidden="true">&times;</button>

											<h4 class="modal-title" id="myModalLabel">Datos del Paciente</h4>
											<form action='/paciente/postPaciente' method='post'>

												<div class="row">
													<div class="col-lg-4">Historial Clinico:<br>
														<input type='text' name='Historial'><br>
													</div>
													<div class="col-lg-4">Apellido Paterno:<br>
														<input type='text' name='apellidop' class='upper'><br>
													</div>
													<div class="col-lg-4">Apellido Materno:<br>
														<input type='text' name='apellidom'><br>
													</div>

												</div>

												<div class="row">
													<div class="col-lg-4">Nombre(s):<br>
														<input type='text' name='nombre'><br>
													</div>
													<div class="col-lg-4">C.I.:<br>
														<input type='text' name='ci'><br>
													</div>
													<div class="col-lg-4">Direcci√≥n:<br>
														<input type='text' name='direccion'><br>
													</div>

												</div>
												<div class="row">

													<div class="col-lg-4">Sexo:<br>
														<select name='sexo'>
															<option>Hombre</option>
															<option>Mujer</option>
														</select><br>
													</div>
													<div class="col-lg-4">Fecha de Nacimiento:<br>
														<input type='date' name='fechanacimiento'><br>
													</div>

												</div>
												<div class="modal-footer">
													<button type="button" class="btn btn-default"
														data-dismiss="modal">Close</button>
													<button type='reset' class='btn btn-info'
														style='margin-right: 20px;'><i class='zmdi zmdi-roller'></i>
														&nbsp;&nbsp; Limpiar</button>
													<button type='submit' class='btn btn-danger'><i
															class='zmdi zmdi-floppy'></i> &nbsp;&nbsp;
														Guardar</button>
												</div>
											</form>
										</div>
									</div>
								</div>
							</div>



						</div>

					</div>

