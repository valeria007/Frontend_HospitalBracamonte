							<!-- Content page -->
							<div class="container-fluid">
								<div class="">
									<h1 class="text-titles"><i class="fa fa-list-alt fa-hc-fw"></i> Citas</h1>
								</div>
							</div>
				
							<div id="main-content">
								<div class="page-content">


									<div class="row">
										<div class="col-md-12">
											<div class="block-web">
												<div class="header">
													<div class="actions"> 
														<a class="minimize" href="#"><i
																class="fa fa-chevron-down"></i></a> <a class="refresh"
															href="#"><i class="fa fa-repeat"></i></a> <a
															class="close-down" href="#"><i class="fa fa-times"></i></a>
													</div>
													
													<a href="javascript:void(0);" class="btn btn-primary btn-raised btn-lg" data-toggle="modal" data-target="#myModal">
															<i class="fa fa-plus-square"></i> <span> Nuevo Paciente</span> </a>
							
													
														<button type="button" value="Imprimir" onclick="window.print()"
															class="btn btn-success btn-raised btn-lg"><i class="zmdi zmdi-print"></i> &nbsp;&nbsp;
															Imprimir</button>
												
												</div>
												<div class="porlets-content">

													<div class="table-responsive">
														<table cellpadding="0" cellspacing="0" border="0"
															class="display table table-bordered" id="hidden-table-info">
															<thead>
																<tr>
																	<th class="text-center">#</th>
																	<th>Historial Clinico</th>
																	<th>Nombres</th>
																	<th>Apellido</th>
																	<th>CI.</th>
																	<th class="hidden-phone">Fichaje</th>
																	<th class="hidden-phone">Reconsulta</th>
																	<th class="hidden-phone">Ver Fichas</th>
																</tr>
															</thead>
															<tbody>

																
																<% for(var i=0; i < resp.length; i++) { %>
																<tr class="gradeA">
																	<td><%= i + 1 %></td>
																	<td><%=resp[i].numeroHistorial%></td>
																	<td><%=resp[i].nombre %></td>
																	<td><%=resp[i].apellidop %></td>
																	<td><%=resp[i].ci %></td>
																	<td class="text-center"><a
																			href="/paciente/citaPAciente/<%= resp[i].id %>/<%= resp[i].numeroHistorial %>"
																			class="btn btn-success "><i
																				class="fa fa-list-ol"></i></a>
																	</td>
																	<td class="text-center"><a
																			href="/paciente/EnviarCita/<%= resp[i].id %>/<%= resp[i].numeroHistorial %>"
																			class="btn btn-info btn-raised btn-xs"><i
																				class="fa fa-retweet"></i></a>
																	</td>
																	<td class="text-center"><a
																			href="/paciente/EnviarCita/<%= resp[i].id %>/<%= resp[i].numeroHistorial %>"
																			class="btn btn-danger btn-raised btn-xs"><i
																				class="fa fa-eye"></i></a>
																	</td>
																</tr>
																<% } %>


															</tbody>
														</table>
													</div>
													<!--/table-responsive-->

												</div>
												<!--/porlets-content-->
											</div>
											<!--/block-web-->
										</div>
										<!--/col-md-12-->
									</div>
									<!--/row-->

								</div>
